{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i ./styles/index.css -o app/styles/app.css",
    "build": "yarn build:css && yarn build:remix",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -w -i ./styles/index.css -o app/styles/app.css",
    "dev": "run-p \"dev:*\"",
    "prod": "yarn build && yarn start",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@remix-run/node": "^1.10.0",
    "@remix-run/react": "^1.10.0",
    "@remix-run/serve": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "isbot": "^3.6.5",
    "prisma": "^4.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@remix-run/dev": "^1.10.0",
    "@remix-run/eslint-config": "^1.10.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.46.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
